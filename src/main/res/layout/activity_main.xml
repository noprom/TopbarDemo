<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:noprom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">



    <com.noprom.mytopbar.Topbar
        android:id="@+id/topbar"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        noprom:leftBackground="@drawable/abc_btn_radio_material"
        noprom:leftText="返回"
        noprom:leftTextColor="#FFFFFF"
        noprom:rightBackground="@drawable/abc_btn_radio_material"
        noprom:rightText="更多"
        noprom:rightTextColor="#FFFFFF"
        noprom:title="我是noprom"
        noprom:titleTextColor="#000000"
        noprom:titleTextSize="10sp"
        >

        </com.noprom.mytopbar.Topbar>


</RelativeLayout>
